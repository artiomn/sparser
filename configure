$as_echo "$gt_cv_int_divbyzero_sigfpe" >&6; }
  case "$gt_cv_int_divbyzero_sigfpe" in
    *yes) value=1;;
    *) value=0;;
  esac

cat >>confdefs.h <<_ACEOF
#define INTDIV0_RAISES_SIGFPE $value
